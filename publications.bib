@article{Cox2017b,
  title = {The Effect of Anger Expression Style on Cardiovascular Responses to Lateralized Cognitive Stressors},
  author = {Cox, David E. and DeVore, Benjamin B. and Harrison, Patti Kelly and Harrison, David W.},
  date = {2017-12-01},
  journaltitle = {Brain Informatics},
  volume = {4},
  number = {4},
  pages = {231--239},
