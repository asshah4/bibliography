  booktitle = {American {{Journal}} of {{Cardiology}}},
  author = {Wei, Jingkai and Rooks, Cherie and Ramadan, Ronnie and Shah, Amit J and Bremner, J. Douglas and Quyyumi, Arshed A and Kutner, Michael and Vaccarino, Viola},
  date = {2014},
  volume = {114},
  number = {2},
  eprint = {24856319},
