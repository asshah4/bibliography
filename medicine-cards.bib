issn = {1475925X},
journal = {BioMedical Engineering Online},
pages = {1--22},
pmid = {22913792},
title = {{Oscillometric measurement of systolic and diastolic blood pressures validated in a physiologic mathematical model}},
volume = {11},
year = {2012}
}
