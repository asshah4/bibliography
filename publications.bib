  author = {Schuetz, Philipp and DLeuppi, Jörg and Bingisser, Roland and Bodmer, Michael and Briel, Matthias and Drescher, Tilman and Duerring, Ursula and Henzen, Christoph and Leibbrandt, Yolanda and Maier, Sabrina and Miedinger, David and Mueller, Beat and Scherr, Andreas and Schindler, Christian and Stoeckli, Rolf and Viatte, Sebastien and Von Garnier, Christophe and Tamm, Michael and Rutishauser, Jonas},
