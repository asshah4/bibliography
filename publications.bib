  author = {Carney, Robert M. and Howells, William B. and Blumenthal, James A. and Freedland, Kenneth E. and Stein, Phyllis K. and Berkman, Lisa F. and Watkins, Lana L. and Czajkowski, Susan M. and Steinmeyer, Brian and Hayano, Junichiro and Domitrovich, Peter P. and Burg, Matthew M. and Jaffe, Allan S.},
  date = {2007},
  journaltitle = {Psychosomatic Medicine},
  volume = {69},
  number = {1},
  eprint = {17167127},
  eprinttype = {pmid},
  pages = {4--9},
  issn = {00333174},
  doi = {10.1097/01.psy.0000249733.33811.00},
  abstract = {OBJECTIVE Depression is a risk factor for mortality after acute myocardial infarction (AMI), possibly as a result of altered autonomic nervous system (ANS) modulation of heart rate (HR) and rhythm. The purposes of this study were to determine: a) whether depressed patients are more likely to have an abnormal HR response (i.e., abnormal turbulence) to premature ventricular contractions (VPCs), and b) whether abnormal HR turbulence accounts for the effect of depression on increased mortality after AMI. METHODS Ambulatory electrocardiographic data were obtained from 666 (316 depressed, 350 nondepressed) patients with a recent AMI; 498 had VPCs with measurable HR turbulence. Of these, 260 had normal, 152 had equivocal, and 86 had abnormal HR turbulence. Patients were followed for up to 30 (median = 24) months. RESULTS Depressed patients were more likely to have abnormal HR turbulence (risk factor adjusted odds ratio = 1.8; 95\% confidence interval [CI] = 1.0-3.0; p = .03) and have worse survival (odds ratio = 2.4; 95\% CI = 1.2-4.6; p = .02) than nondepressed patients. When HR turbulence was added to the model, the adjusted hazard ratio for depression decreased to 1.9 (95\% CI = 0.9-3.8; p = .08), and to 1.6 (95\% CI = 0.8-3.4; p = .18) when a measure of HR variability (LnVLF) was added. The hazard was found to differ over time with depression posing little risk for mortality in year 1 but greater risk in years 2 and 3 of the follow up. CONCLUSION ANS dysregulation may partially mediate the increased risk for mortality in depressed patients with frequent VPCs after an AMI.},
  isbn = {0033-3174},
  keywords = {Acute myocardial infarction,Depression,Heart rate turbulence,Survival},
  file = {C\:\\Users\\asshah4\\projects\\zotero\\storage\\VQ6CU4VC\\2007 - Carney et al. - Heart rate turbulence, depression, and survival after acute myocardial infarction.pdf}
}

