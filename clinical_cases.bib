pages = {191--200},
pmid = {25205644},
title = {{Adaptive choice of patient subgroup for comparing two treatments}},
url = {http://linkinghub.elsevier.com/retrieve/pii/S1551714414001311 http://www.ncbi.nlm.nih.gov/pubmed/25205644 http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=PMC4470707},
