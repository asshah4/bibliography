  author = {Marcus, Gregory M. and Smith, Lisa M. and Whiteman, Dean and Tseng, Zian H. and Badhwar, Nitish and Lee, Byron K. and Lee, Randall J. and Scheinman, Melvin M. and Olgin, Jeffrey E.},
  date = {2008-03},
  journaltitle = {PACE - Pacing and Clinical Electrophysiology},
  volume = {31},
