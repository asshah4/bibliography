@article{Schwartz2018,
  title = {Modifier Genes for Sudden Cardiac Death},
  author = {Schwartz, Peter J and Crotti, Lia and George, Alfred L},
  date = {2018},
  journaltitle = {European heart journal},
  volume = {39},
