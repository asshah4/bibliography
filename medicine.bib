isbn = {0819405205},
issn = {0301486X},
journal = {Mycopathologia},
keywords = {Cryptococcosis,HBV,HCV,Viral hepatitis},
number = {3-4},
pages = {307--312},
pmid = {25528539},
title = {{Cryptococcosis in Patients Living with Hepatitis C and B Viruses}},
url = {https://link-springer-com.proxy.library.emory.edu/content/pdf/10.1007%2Fs11046-014-9843-4.pdf},
volume = {179},
year = {2015}
