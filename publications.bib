@article{Blase2010,
  title = {Efficacy of {{HRV-biofeedback}} as Additional Treatment of Depression and {{PTSD}}.},
  author = {Blase, K L and family=Dijke, given=A, prefix=van, useprefix=true and Cluitmans, P J M and Vermetten, E},
