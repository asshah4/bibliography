@article{Granada2000,
  title = {Incidence and Predictors of Atrial Flutter in the General Population},
  author = {Granada, Juan and Uribe, William and Chyou, Po Huang and Maassen, Karen and Vierkant, Robert and Smith, Peter N. and Hayes, John and Eaker, Elaine and Vidaillet, Humberto},
