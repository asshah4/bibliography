  author = {Yeragani, Vikram K and Pesce, Vanessa and Jayaraman, Anusha and Roose, Steven and V.K., Yeragani and V., Pesce and A., Jayaraman and S., Roose},
  date = {2002-09-01},
  journaltitle = {Biological psychiatry},
  volume = {52},
  number = {5},
  eprint = {12242058},
  eprinttype = {pmid},
  pages = {418--29},
  issn = {0006-3223},
  url = {http://www.embase.com/search/results?subaction=viewrecord&from=export&id=L35266733%5Cnhttp://dx.doi.org/10.1016/S0006-3223(02)01394-X%5Cnhttp://sfx.library.uu.nl/utrecht?sid=EMBASE&issn=00063223&id=doi:10.1016%2FS0006-3223%2802%2901394-X&atitle=Major+depr},
  abstract = {Background: Studies have linked depression to sudden death and serious cardiovascular events in patients with preexisting cardiac illness. Recent studies have shown decreased vagal function in cardiac patients with depression and depressed patients without cardiac illness. Methods: We compared 20-hour, sleeping, and awake heart period variability measures using spectral analysis, fractal dimension, and symbolic dynamics in two patient groups with major depression and ischemic heart disease (mean age 59-60 years) before and after 6 weeks of paroxetine or nortriptyline treatment. Results: Spectral measures showed decreases in awake and sleeping total power (TP: 0.0-0.5 Hz), ultra low frequency power (ULF: 0-0.0033 Hz), very low frequency power (VLF: 0.0033-0.04 Hz), and low-frequency power (LF: 0.04-0.15 Hz) for nortriptyline condition and a decrease in high-frequency power (HF: 0.15-0.5 Hz) for the awake condition in patients who received nortriptyline. A measure of nonlinear complexity, WC-100, significantly increased after paroxetine during the awake condition. Conclusions: These findings suggest that nortriptyline has stronger vagolytic effects on cardiac autonomic function compared with paroxetine, which is in agreement with previous clinical and preclinical reports. Paroxetine may have some cardio-protective effects, especially in cardiac patients. © 2002 Society of Biological Psychiatry.},
  keywords = {adult,aged,article,awake,cardiovascular mortality,clinical trial,controlled clinical trial,controlled study,depression,disease association,double blind procedure,drug blood level,drug effect,electrocardiogram,fractal analysis,heart protection,heart rate variability,hemodynamic monitoring,human,ischemic heart disease,major clinical study,Major depression,nonlinear,nortriptyline,paroxetine,priority journal,sleep,sleep waking cycle,spectral analysis,symbolic dynamics,treatment outcome,vagus tone},
