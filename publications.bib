@article{Fischer2016,
  title = {A Novel Method to Visualise and Quantify Circadian Misalignment},
  author = {Fischer, Dorothee and Vetter, Céline and Roenneberg, Till},
  date = {2016-12-08},
  journaltitle = {Scientific Reports},
