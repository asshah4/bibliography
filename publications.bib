@article{Arruda1998,
  title = {Development and Validation of an {{ECG}} Algorithm for Identifying Accessory Pathway Ablation Site in {{Wolff-Parkinson-White}} Syndrome},
  author = {Arruda, Mauricio S and Mcclelland, James H and Wang, Xanzhung and Beckman, Karen J and Widman, Lawrence E and Gonzalez, Mario D and Nakagawa, Hiroshi and Lazzara, Ralph and Jackman, Warren M},
  date = {1998},
  journaltitle = {Journal of Cardiovascular Electrophysiology},
  volume = {9},
  number = {1},
