@article{Halligan2004,
  title = {The {{Natural History}} of {{Lone Atrial Flutter}}},
  author = {Halligan, Sean C. and Gersh, Bernard J. and Brown, Robert D. and Rosales, A. Gabriela and Munger, Thomas M. and Shen, Win Kuang and Hammill, Stephen C. and Friedman, Paul A.},
