  author = {family=Wyse, given=DG, given-i=DG and family=Waldo, given=AL, given-i=AL and JP, DiMarco and family=Domanski, given=MJ, given-i=MJ and family=Schron, given=EB, given-i=EB and Rosenberg, Y and family=Kellen, given=JC, given-i=JC and family=Greene, given=HL, given-i=HL and family=Mickel, given=MC, given-i=MC and family=Dalquist, given=JE, given-i=JE and family=Corley, given=SD, given-i=SD},
  date = {2002-12-05},
