title = {{Obstructive sleep apnea is associated with non-sustained ventricular tachycardia in patients with hypertrophic obstructive cardiomyopathy}},
url = {https://www.sciencedirect.com/science/article/pii/S1547527118312839?dgcid=rss_sd_all https://linkinghub.elsevier.com/retrieve/pii/S1547527118312839 http://www.ncbi.nlm.nih.gov/pubmed/30576881},
