@article{Brooks2014,
  title = {Social Relationships and Allostatic Load in the Midus Study},
  author = {Brooks, Kathryn P. and Gruenewald, Tara and Karlamangla, Arun and Hu, Peifung and Koretz, Brandon and Seeman, Teresa E.},
  date = {2014},
  journaltitle = {Health Psychology},
