  title = {Recommendations for {{Multimodality Cardiovascular Imaging}} of {{Patients}} with {{Hypertrophic Cardiomyopathy}}: {{An Update}} from the {{American Society}} of {{Echocardiography}}, in {{Collaboration}} with the {{American Society}} of {{Nuclear Cardiology}}, the {{Society}} for {{Cardiovascular Magnetic Resonance}}, and the {{Society}} of {{Cardiovascular Computed Tomography}}},
  author = {Nagueh, Sherif F. and Phelan, Dermot and Abraham, Theodore and Armour, Alicia and Desai, Milind Y. and Dragulescu, Andreea and Gilliland, Yvonne and Lester, Steven J. and Maldonado, Yasdet and Mohiddin, Saidi and Nieman, Koen and Sperry, Brett W. and Woo, Anna},
