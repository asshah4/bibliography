@article{Lehrer2003,
  title = {Heart Rate Variability Biofeedback Increases Baroreflex Gain and Peak Expiratory Flow},
  author = {Lehrer, Paul M. and Vaschillo, Evgeny and Vaschillo, Bronya and Lu, Shou En and Eckberg, Dwain L. and Edelberg, Robert and Shih, Weichung Joe and Lin, Yong and Kuusela, Tom A. and Tahvanainen, Kari U.O. and Hamer, Robert M.},
