issn = {2296-889X},
journal = {Frontiers in Molecular Biosciences},
keywords = {12,algorithm,b 12,cbl,cobalamin,diagnostic,diagnostic algorithm,functional deficiency of B12,functional deficiency of b,holo-transcobalamin,homocysteine,is an essential water-soluble,methylmalonic acid,the chemical name,vitamin B12,vitamin b 12,vitamin b 12 deficiency},
month = {jun},
