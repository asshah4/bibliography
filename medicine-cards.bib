doi = {10.1161/CIRCULATIONAHA.106.613596},
file = {:Users/shahbook/Box Sync/Mendeley_Articles/2008 - Reynolds, Hochman - Cardiogenic shock current concepts and improving outcomes.pdf:pdf},
isbn = {1524-4539 (Electronic) 0009-7322 (Linking)},
