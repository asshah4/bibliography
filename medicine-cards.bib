isbn = {1468-201X},
issn = {13556037},
journal = {Heart},
number = {16},
pages = {1246--1254},
pmid = {22826561},
title = {{Percutaneous cardiac support devices for cardiogenic shock: Current indications and recommendations}},
volume = {98},
