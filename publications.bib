}

@inproceedings{Boudreau2011,
  title = {Correlation of Heart Rate Variability and Circadian Markers in Humans},
  booktitle = {2011 {{Annual International Conference}} of the {{IEEE Engineering}} in {{Medicine}} and {{Biology Society}}},
  author = {Boudreau, Philippe and Dumont, Guy and Kin, N. M. K Ng Ying and Walker, Claire-Dominique Dominique and Boivin, Diane B.},
  date = {2011-08},
  eprint = {22254401},
