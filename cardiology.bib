@article{Seward2010,
  title = {Infiltrative {{Cardiovascular Diseases}}. {{Cardiomyopathies That Look Alike}}},
  author = {Seward, James B. and Casaclang-Verzosa, Grace},
  date = {2010-04-27},
  journaltitle = {Journal of the American College of Cardiology},
  volume = {55},
