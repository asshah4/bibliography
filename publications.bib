@article{Bunting2021,
  title = {Cardiac {{Imaging}} to {{Assess Left Ventricular Systolic Function}} in {{Atrial Fibrillation}}},
  author = {Bunting, Karina V. and O'Connor, Kieran and Steeds, Richard P. and Kotecha, Dipak},
  date = {2021-01-15},
