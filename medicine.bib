journal = {Clinical Immunology},
keywords = {C-reactive protein,D-dimer,antiretroviral therapy,biomarker,immune reconstitution inflammatory syndrome,immune restoration disease,paradoxical IRIS,unmasking IRIS},
month = {jul},
number = {1},
pages = {42--50},
