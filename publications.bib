  author = {Schosser, Alexandra and Butler, Amy W. and Uher, Rudolf and Ng, Mandy Y. and Cohen-Woods, Sarah and Craddock, Nick and Owen, Mike J. and Korszun, Ania and Gill, Michael and Rice, John and Hauser, Joanna and Henigsberg, Neven and Maier, Wolfgang and Mors, Ole and Placentino, Anna and Rietschel, Marcella and Souery, Daniel and Preisig, Martin and Craig, Ian W. and Farmer, Anne E. and Lewis, Cathryn M. and McGuffin, Peter},
