@article{Manuck1988,
  title = {Effects of Stress and the Sympathetic Nervous System on Coronary Artery Atherosclerosis in the Cynomolgus Macaque},
  author = {Manuck, Stephen B. and Kaplan, Jay R. and Adams, Michael R. and Clarkson, Thomas B.},
