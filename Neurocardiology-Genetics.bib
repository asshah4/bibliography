issn = {0195668X},
journal = {European Heart Journal},
keywords = {Asymmetric dimethylarginine,Genetics,Genome-wide association study,Heart rate variability,Mortality,Sudden cardiac death,Symmetric dimethylarginine},
month = {feb},
number = {8},
pages = {524--530},
pmid = {24159190},
publisher = {Oxford University Press},
title = {{Genome-wide association study on dimethylarginines reveals novel AGXT2 variants associated with heart rate variability but not with overall mortality}},
