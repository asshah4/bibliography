@article{Kaydu2018,
  title = {Preoperative and {{Postoperative Assessment}} of {{Ultrasonographic Measurement}} of {{Inferior Vena Cava}}: {{A Prospective}}, {{Observational Study}}},
  author = {Kaydu, Ayhan and Gokcek, Erhan},
  date = {2018-06-10},
  journaltitle = {Journal of Clinical Medicine},
  volume = {7},
