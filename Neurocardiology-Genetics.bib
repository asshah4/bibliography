pmid = {30180964},
title = {{Associations between polymorphisms of the CXCL12 and CNNM2 gene and hypertension risk: A case-control study}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/30180964 https://linkinghub.elsevier.com/retrieve/pii/S0378111918307698},
