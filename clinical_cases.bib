issn = {20721439},
journal = {Journal of Thoracic Disease},
keywords = {Lung,Pulmonary fibrosis,Tumorlet},
month = {dec},
number = {6},
pages = {655--658},
pmid = {23205296},
publisher = {AME Publications},
title = {{Pulmonary carcinoid tumorlet without underlying lung disease: Analysis of its relationship to fibrosis}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/23205296 http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=PMC3506797},
volume = {4},
