@article{Perez2016,
  title = {Evaluation and {{Management}} of {{Tolosa}}–{{Hunt Syndrome}} in {{Children}}: {{A Clinical Update}}},
  author = {Pérez, Carlos A and Evangelista, Monaliza},
  date = {2016},
  journaltitle = {Pediatric Neurology},
  volume = {62},
