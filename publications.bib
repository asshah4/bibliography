@article{shu2007,
  title = {Foxp2 and {{Foxp1}} Cooperatively Regulate Lung and Esophagus Development},
  author = {Shu, Weiguo and Lu, Min Min and Zhang, Yuzhen and Tucker, Philip W and Zhou, Deying and Morrisey, Edward E},
  date = {2007-05},
  journaltitle = {Development (Cambridge, England)},
  shortjournal = {Development},
  volume = {134},
  number = {10},
