  title = {Cryptococcosis in {{Patients Living}} with {{Hepatitis C}} and {{B Viruses}}},
  author = {Spies, Fernanda Sá and family=Oliveira, given=Markus Berger, prefix=de, useprefix=true and Krug, Monique Siebra and Severo, Cecilia Bittencourt and Severo, Luiz Carlos and Vainstein, Marilene Henning},
